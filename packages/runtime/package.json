{
  "name": "@openapi-io-ts/runtime",
  "version": "0.3.1",
  "description": "Runtime for openapi-io-ts",
  "keywords": [
    "openapi",
    "io-ts",
    "codegen",
    "code generation",
    "swagger",
    "validation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Fredx87/openapi-io-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fredx87/openapi-io-ts.git",
    "directory": "packages/runtime"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --sourcemap --legacy-output"
  },
  "dependencies": {
    "@openapi-io-ts/core": "workspace:*"
  },
  "peerDependencies": {
    "fp-ts": "^2.11.0",
    "io-ts": "^2.2.0"
  }
}
